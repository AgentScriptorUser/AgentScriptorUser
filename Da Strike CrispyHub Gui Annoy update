    loadstring(game:HttpGet('https://pastebin.com/raw/3Rnd9rHf'))()  
end)

end)

-- Connect Home tab to function
homeTab.MouseButton1Click:Connect(loadHomeTab)

-- Call it on GUI load
loadHomeTab()
