punchSound.SoundId = soundId
punchSound.Volume = 1
punchSound.Looped = true -- Make the sound loop
punchSound.Parent = character:WaitForChild("HumanoidRootPart")

-- Play animation and sound
animationTrack:Play()
animationTrack.Looped = true -- Super Punch usually plays once

punchSound:Play() -- Play the punch sound in loop
        end
